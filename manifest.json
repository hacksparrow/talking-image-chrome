{
  "manifest_version": 2,
  "name": "Talking Image",
  "version": "0.1.2",
  "description": "Play audio from images with appended Ogg / Mp3 files",
  "author": "Hage Yaapa",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jdataview.js",
        "jbinary.js",
        "talking-image.js"
      ]
    }
  ],
  "icons": {
    "128": "icon-128.png",
    "112": "icon-112.png",
    "96": "icon-96.png",
    "48": "icon-48.png",
    "16": "icon-16.png"
  },
  "homepage_url": "https://github.com/hacksparrow/talking-image-chrome"
}